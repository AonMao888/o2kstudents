<%if(all.length !== 0){%>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>O2K <%=page.toUpperCase()%> Lists <%=year%></title>
    <link rel="shortcut icon" href="../../logo" type="image/x-icon">
    <meta name="description" content="O2K <%=page.toUpperCase()%> student's list with information in this page.">
    <link rel="stylesheet" href="../t.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">
</head>
<body>
    <nav>
        <div class="left">
            <i class="fa-solid fa-bars menubtn" onclick="showmenu()"></i>
            <a href="../"><i class="fa-solid fa-house"></i></a>
            <a href="" class="logo">O2K <%=page.toUpperCase()%><span>List <%=year%></span></a>
        </div>
        <div class="right">
            <a href="../scanner"><i class="fa-solid fa-camera"></i><p>Scanner</p></a>
            <a href="javascript:shar()"><i class="fa-solid fa-share-nodes"></i><p>Share</p></a>
        </div>
    </nav>
    <div class="con">
        <div class="menu">
            <a href="../students/<%=year%>" class="<%if(page == 'Students'){%>active<%}%>"><img src="../../imgall" alt=""><p>All students</p></a>
            <a href="../teachers/<%=year%>" class="<%if(page == 'Teachers'){%>active<%}%>"><img src="../../imgteacher" alt=""><p>Teachers</p></a>
            <a href="../ged/<%=year%>" class="<%if(page == 'ged'){%>active<%}%>"><img src="../../imgged" alt=""><p>GED</p></a>
            <a href="../pre-ged/<%=year%>" class="<%if(page == 'pre ged'){%>active<%}%>"><img src="../../imgpreged" alt=""><p>Pre GED</p></a>
            <a href="../secondary/<%=year%>" class="<%if(page == 'secondary'){%>active<%}%>"><img src="../../imgsecondary" alt=""><p>Secondary</p></a>
        </div>
        <div class="home">
            <h1>All <%=page.toUpperCase()%> List<span><%=all.length%></span></h1>
            <div class="sc">
                <table cellspacing="0">
                    <tr>
                        <th>Name</th>
                        <th>Class</th>
                        <th>Address</th>
                        <th>Email</th>
                        <th>Student ID</th>
                        <th>Points</th>
                    </tr>
                    <%all.forEach((item)=>{%>
                        <tr>
                            <td><%=item.name%></td>
                            <td><%=item.class.toUpperCase()%></td>
                            <td><%=item.address%></td>
                            <td><%=item.email%></td>
                            <td><a href="../p/<%=item.member_id%>"><%=item.member_id.toUpperCase()%></a></td>
                            <%if(item.class == 'ged'){%>
                            <td><%= Number(item.english)+Number(item.mathematic)+Number(item.science)+Number(item.thai)+Number(item.social)+Number(item.rla)%></td>
                            <%}else if(item.class == 'pre ged'){%>
                            <td><%=Number(item.tai)+Number(item.burmese)+Number(item.english)+Number(item.mathematic)+Number(item.science)+Number(item.thai)+Number(item.art)+Number(item.chinese)%></td>
                            <%}else if(item.class == 'secondary'){%>
                            <td><%=Number(item.tai)+Number(item.burmese)+Number(item.english)+Number(item.mathematic)+Number(item.science)+Number(item.thai)+Number(item.art)+Number(item.chinese)%></td>
                            <%}%>
                        </tr>
                    <%})%>
                </table>
            </div>
        </div>
    </div>
    <script>
        var menu = document.querySelector(".menu");
        function showmenu(){menu.style.left=0}
        document.querySelector(".home").onclick=()=>{menu.style.left='-300px'}
        document.onscroll=()=>{menu.style.left='-300px'}
        function shar(){navigator.share({title:document.title,url:window.location.href})}
    </script>
</body>
</html>
<%}%>